## @todo export: why is this?
/CryptoPkg/Library/OpensslLib/openssl*: --svn-sync-process-export none
## @todo export: slipup?
/MdePkg/Library/BaseFdtLib/libfdt/libfdt/.gitignore: --svn-sync-process-export none


# Reset the directory filtering so we can process MdeModulePkg/Universal/ReportStatusCodeRouter/.
--filter-out-dirs ""

# Most of this is 3rd party stuff.
/*!xcpt!/.scm-settings|/*.kmk|/MdeModulePkg/Universal/Disk/PartitionDxe/Apple.c|/VBox*|/*/VBox*: \
        --external-copyright --dont-set-svn-keywords --no-convert-tabs --no-strip-trailing-blanks \
        --strip-no-trailing-lines --no-force-final-eol --no-fix-todos --no-fix-header-guards

/MdeModulePkg/Universal/Disk/PartitionDxe/Apple.c:  --license-ose-dual
/*.kmk|/VBox*|/*/VBox*:                             --license-ose-dual

# Build related files using '#' for comments.
/*.dec:        --treat-as Makefile
/*.dsc:        --treat-as Makefile
/*.fdf:        --treat-as Makefile
/*.inf:        --treat-as Makefile
/BaseTools/Conf/build_rule.template:   --treat-as Makefile
/BaseTools/Conf/target.template:       --treat-as Makefile
/BaseTools/Conf/tools_def.template:    --treat-as Makefile

# IASL sources.
/*.asl:        --treat-as *.h
/*.aslc:       --treat-as *.h

# NASM source.
/*.nasm:       --treat-as .asm
/*.nasmb:      --treat-as .asm
/*.asm16:      --treat-as .asm

# cpp processed assembly files.
/*.s:          --treat-as .h
/*.i:          --treat-as .h

# GAS assembly files w/o preprocessing.
/*.S:          --treat-as Makefile

# Python sources.
/*.pyw:        --treat-as python

# Not quite sure what this is, uses C++ comment style.
/*.vfr:        --treat-as .h
/*.Vfr:        --treat-as .h

# Linker scripts.
/*-ld-script:  --treat-as .h
/*.lds:        --treat-as .h

# Grammar files. Treat as C for now.
/*.g:          --treat-as .c

# Not entirely sure what the .dlg files are, but they seems to contain C like license headers.
/*.dlg:        --treat-as .h

# Misc.
*.xsd:                  --treat-as .xml
/*.ini:        --treat-as Makefile
/BaseTools/Conf/FrameworkDatabase.template:     --treat-as xml
/BaseTools/Source/C/VfrCompile/Pccts/*.r:       --treat-as h
/BaseTools/BuildEnv:                            --treat-as shell
/BaseTools/Bin/GccLto/*.a:                      --treat-as binary
/BaseTools/Source/Python/Ecc/CParser4/C.g4:     --treat-as c --no-convert-eol

# Files that aren't UTF-8.
