# The whole lot is dual licensed.
--license-ose-dual

# Exporting everything.
--svn-sync-process-export all

/DRM/include/*: --3rd-party

/Installer/vboxguest.depend:    --treat-as plaintext --del-action force-native-eol --add-action force-lf
/Installer/vboxguest.pkginfo:   --treat-as plaintext --del-action force-native-eol --add-action force-lf
/Installer/vboxguest.space:     --treat-as hashtext  --del-action force-native-eol --add-action force-lf --external-copyright
/Installer/postinstall.sh:      --no-convert-tabs --license-ose-dual
/Installer/vbox_vendor_select:  --treat-as shell --license-based-on-mit --no-convert-tabs

/Mouse/vboxms.conf:             --treat-as shell

/SharedFolders/*:               --no-convert-tabs
/SharedFolders/solaris10/*:     --svn-sync-process-export none
/SharedFolders/solaris10/sys/*: --3rd-party

/DRM/vboxvideo_drm.c: --no-convert-tabs

